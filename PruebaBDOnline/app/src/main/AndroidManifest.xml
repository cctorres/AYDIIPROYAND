    <uses-permission
        android:name="android.permission.INTERNET"
        android:maxSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Vista.PruebaMesa"
            android:label="@string/title_activity_prueba_mesa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Vista.inicioSesion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Vista.VistaAdmin" />
        <activity android:name=".Vista.VistaRestaurante" />
        <activity android:name=".Vista.EmpleadosVista" />
        <activity android:name=".Vista.EmpleadoDetalle"></activity>
    </application>

</manifest>